#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

#define DEFAULT 0

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
        &kp 1 &kp 2 &kp 3 &kp 4  &kp 5          &trans                               &trans    &kp 6     &kp 7  &kp 8     &kp 9   &kp 0
&kp TAB &kp Q &kp W &kp F &kp P  &kp G          &trans                               &trans    &kp J     &kp L  &kp U     &kp Y   &kp SEMI      &kp APOS
&kp ESC &kp A &kp R &kp S &kp T  &kp D          &trans                               &trans    &kp H     &kp N  &kp E     &kp I   &kp O         &kp MINUS
&kp DEL &kp Z &kp X &kp C &kp V  &kp B                                                         &kp K     &kp M  &kp COMMA &kp DOT &kp BACKSLASH &kp PLUS
                                                &trans       &trans         &trans   &trans
                          &trans &kp LEFT_SHIFT &kp LEFT_WIN &kp LCTRL      &kp RALT &kp ENTER &kp SPACE &trans

            >;
        };
    };
};
